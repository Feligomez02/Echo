# Production environment variables for Vercel build phase
# These are needed during 'prisma generate' and 'next build'
# The actual DATABASE_URL will be injected by Vercel from dashboard

# Placeholder for build phase - Vercel will override this
DATABASE_URL="postgresql://placeholder:placeholder@localhost:5432/placeholder"

# NextAuth URL for production
NEXTAUTH_URL="https://your-app.vercel.app"

# NextAuth Secret - will be set in Vercel dashboard
NEXTAUTH_SECRET="your-secret-here"
